        </xsd:simpleContent>
    </xsd:complexType>
    
    <xsd:complexType name="VPN_Type">
        <xsd:simpleContent>
            <xsd:extension base="xsd:string">
                <xsd:attribute type="xsd:int"    name="vpnId"/>
