<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" jaxb:version="1.0">
    <xsd:annotation>
        <xsd:appinfo>
            <jaxb:globalBindings generateIsSetMethod="false" optionalProperty="primitive"/>
            <jaxb:schemaBindings>
                <jaxb:package name="es.tena.vodafone.factura"/>
            </jaxb:schemaBindings>
            <!--jaxb:bindings node="//xsd:complexType[@name='TOT_Type']">
                <jaxb:class ref="es.tena.vodafone.facturas.TOT_Type_"/>
            </jaxb:bindings>
            <jaxb:bindings node=".//xsd:element[@name='TOT']">
                <jaxb:class ref="es.tena.vodafone.facturas.TOT_Type_"/>
            </jaxb:bindings-->
            
        </xsd:appinfo>
    </xsd:annotation>
  
    <xsd:annotation>
        <xsd:documentation xml:lang="en">
            Facturas de vodafone
        </xsd:documentation>
    </xsd:annotation>
  
    <xsd:element name="INVOICE" type="INVOICE_Type" />
    
    <xsd:complexType name="INSTACCNT_Type">
        <xsd:sequence>
            <xsd:element name="INFO" type="INFO_Type" />
            <xsd:element name="TOT" type="TOT_Type" />
            <xsd:element name="SERVSUMMARY" type="SERVSUMMARY_Type" />
            <xsd:element name="NOTAXSUMM" type="NOTAXSUMM_Type" />                                
            <xsd:element name="SERV" maxOccurs="unbounded" minOccurs="0" type="SERV_Type" />
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="INVOICE_Type">
        <xsd:sequence>
            <xsd:element name="INFO" type="INFO_Type" />
            <xsd:element name="TOT" type="TOT_Type" />
            <xsd:element name="SERVSUMMARY" type="SERVSUMMARY_Type" />
            <xsd:element name="ACCNTCHARGES" type="ACCNTCHARGES_Type" />
            <xsd:element name="NOTAXSUMM" type="NOTAXSUMM_Type" />
            <xsd:element name="SERVTOTALSUMM" type="SERVTOTALSUMM_Type" />
            <xsd:element name="CREDITS" type="CREDITS_Type" />
            <xsd:element name="INSTACCNT" type="INSTACCNT_Type" />
        </xsd:sequence>
    </xsd:complexType>
   

    <xsd:complexType name="BILL_Type">
        <xsd:simpleContent>
            <xsd:extension base="xsd:string">
                <xsd:attribute type="xsd:date" name="toDt"/>
                <xsd:attribute type="xsd:date" name="statementDt"/>
                <xsd:attribute type="xsd:int" name="sfid"/>
                <xsd:attribute type="xsd:string" name="promotionId"/>
                <xsd:attribute type="xsd:byte" name="preNumAccnt"/>
                <xsd:attribute type="xsd:string" name="preDocType"/>
                <xsd:attribute type="xsd:int" name="numAccnt"/>
                <xsd:attribute type="xsd:short" name="monoLine"/>
                <xsd:attribute type="xsd:byte" name="idLang"/>
                <xsd:attribute type="xsd:date" name="fromDt"/>
                <xsd:attribute type="xsd:byte" name="formatOpt"/>
                <xsd:attribute type="xsd:byte" name="emitPlaceID"/>
                <xsd:attribute type="xsd:date" name="emitDt"/>
                <xsd:attribute type="xsd:date" name="dueDt"/>
                <xsd:attribute type="xsd:string" name="docType"/>
                <xsd:attribute type="xsd:string" name="descEmitPlace"/>
                <xsd:attribute type="xsd:string" name="descEmitCountry"/>
                <xsd:attribute type="xsd:float" name="defTaxRate"/>
                <xsd:attribute type="xsd:dateTime" name="dateActive"/>
                <xsd:attribute type="xsd:byte" name="currCode"/>
                <xsd:attribute type="xsd:string" name="currCSap"/>
                <xsd:attribute type="xsd:byte" name="courtesyBill"/>
                <xsd:attribute type="xsd:byte" name="countryCode"/>
                <xsd:attribute type="xsd:string" name="billType"/>
                <xsd:attribute type="xsd:byte" name="billSeqNum"/>
                <xsd:attribute type="xsd:byte" name="billRefResets"/>
                <xsd:attribute type="xsd:string" name="billRef"/>
                <xsd:attribute type="xsd:string" name="billPeriod"/>
                <xsd:attribute type="xsd:int" name="fcnumAccnt"/>
                <xsd:attribute type="xsd:byte" name="accountCategory"/>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
  
    <xsd:complexType name="CUST_Type">
        <xsd:simpleContent>
            <xsd:extension base="xsd:string">
                <xsd:attribute type="xsd:string" name="treat"/>
                <xsd:attribute type="xsd:string" name="title"/>
                <xsd:attribute type="xsd:byte"   name="robinsonFlag"/>
                <xsd:attribute type="xsd:string" name="nifCif"/>
                <xsd:attribute type="xsd:string" name="name"/>
                <xsd:attribute type="xsd:string" name="company"/>
                <xsd:attribute type="xsd:string" name="cargo"/>
                <xsd:attribute type="xsd:string" name="IdType"/>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    
    <xsd:complexType name="ADDR_Type">
        <xsd:simpleContent>
            <xsd:extension base="xsd:string">
                <xsd:attribute type="xsd:int" name="zip"/>
                <xsd:attribute type="xsd:string" name="prov"/>
                <xsd:attribute type="xsd:string" name="line3"/>
                <xsd:attribute type="xsd:string" name="line2"/>
                <xsd:attribute type="xsd:string" name="line1"/>
                <xsd:attribute type="xsd:string" name="dest"/>
                <xsd:attribute type="xsd:string" name="country"/>
                <xsd:attribute type="xsd:string" name="city"/>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    
    <xsd:complexType name="PAY_Type">
        <xsd:simpleContent>
            <xsd:extension base="xsd:string">
                <xsd:attribute type="xsd:int" name="reference"/>
                <xsd:attribute type="xsd:string" name="payMethodSap"/>
                <xsd:attribute type="xsd:string" name="payMethodDoc1Desc"/>
                <xsd:attribute type="xsd:byte" name="payMethodDoc1"/>
                <xsd:attribute type="xsd:byte" name="payMethod"/>
                <xsd:attribute type="xsd:string" name="payConditionSap"/>
                <xsd:attribute type="xsd:string" name="controlDigit"/>
                <xsd:attribute type="xsd:string" name="cardCarrier"/>
                <xsd:attribute type="xsd:string" name="cardAcount"/>
                <xsd:attribute type="xsd:string" name="bankSortCode"/>
                <xsd:attribute type="xsd:string" name="bankName"/>
                <xsd:attribute type="xsd:string" name="bankCode"/>
                <xsd:attribute type="xsd:string" name="bankAgencyCode"/>
                <xsd:attribute type="xsd:string" name="bankAccNum"/>
                <xsd:attribute type="xsd:string" name="bankAccName"/>
                <xsd:attribute type="xsd:string" name="agencyCode"/>
                <xsd:attribute type="xsd:string" name="barCode"/>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    
    <xsd:complexType name="MK_Type">
        <xsd:simpleContent>
            <xsd:extension base="xsd:string">
                <xsd:attribute type="xsd:string" name="stuffer"/>
                <xsd:attribute type="xsd:string" name="segment"/>
                <xsd:attribute type="xsd:byte"   name="numberVS"/>
                <xsd:attribute type="xsd:byte"   name="explainFlag"/>
                <xsd:attribute type="xsd:string" name="campaign"/>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    
    <xsd:complexType name="HIST_Type">
        <xsd:simpleContent>
            <xsd:extension base="xsd:string">
                <xsd:attribute type="xsd:float" name="month9"/>
                <xsd:attribute type="xsd:float" name="month8"/>
                <xsd:attribute type="xsd:float" name="month7"/>
                <xsd:attribute type="xsd:float" name="month6"/>
                <xsd:attribute type="xsd:float" name="month5"/>
                <xsd:attribute type="xsd:float" name="month4"/>
                <xsd:attribute type="xsd:float" name="month3"/>
                <xsd:attribute type="xsd:float" name="month2"/>
                <xsd:attribute type="xsd:float" name="month13"/>
                <xsd:attribute type="xsd:float" name="month12"/>
                <xsd:attribute type="xsd:float" name="month11"/>
                <xsd:attribute type="xsd:float" name="month10"/>
                <xsd:attribute type="xsd:float" name="month1"/>
                <xsd:attribute type="xsd:float" name="min9"/>
                <xsd:attribute type="xsd:float" name="min8"/>
                <xsd:attribute type="xsd:float" name="min7"/>
                <xsd:attribute type="xsd:float" name="min6"/>
                <xsd:attribute type="xsd:float" name="min5"/>
                <xsd:attribute type="xsd:float" name="min4"/>
                <xsd:attribute type="xsd:float" name="min3"/>
                <xsd:attribute type="xsd:float" name="min2"/>
                <xsd:attribute type="xsd:byte"  name="min13"/>
                <xsd:attribute type="xsd:float" name="min12"/>
                <xsd:attribute type="xsd:float" name="min11"/>
                <xsd:attribute type="xsd:float" name="min10"/>
                <xsd:attribute type="xsd:float" name="min1"/>
                <xsd:attribute type="xsd:float" name="maxCons"/>
                <xsd:attribute type="xsd:float" name="dayAVG"/>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    
    <xsd:complexType name="LOY_Type">
        <xsd:simpleContent>
            <xsd:extension base="xsd:string">
                <xsd:attribute type="xsd:date" name="procDt"/>
                <xsd:attribute type="xsd:byte" name="noFacCheck"/>
                <xsd:attribute type="xsd:byte" name="loyFlag"/>
                <xsd:attribute type="xsd:byte" name="expPoints3"/>
                <xsd:attribute type="xsd:byte" name="expPoints2"/>
                <xsd:attribute type="xsd:byte" name="expPoints1"/>
                <xsd:attribute type="xsd:date" name="expDt3"/>
                <xsd:attribute type="xsd:date" name="expDt2"/>
                <xsd:attribute type="xsd:date" name="expDt1"/>
