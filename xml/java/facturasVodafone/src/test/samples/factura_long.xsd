        <xsd:attribute type="xsd:float" name="total" use="optional"/>
        <xsd:attribute type="xsd:string" name="time" use="optional"/>
        <xsd:attribute type="xsd:int" name="serv" use="optional"/>
        <xsd:attribute type="xsd:string" name="product" use="optional"/>
        <xsd:attribute type="xsd:byte" name="isVoiceUsage" use="optional"/>
        <xsd:attribute type="xsd:string" name="extIdDOC1" use="optional"/>
        <xsd:attribute type="xsd:string" name="desc" use="optional"/>
        <xsd:attribute type="xsd:date" name="date" use="optional"/>
    </xsd:complexType>
    
    <xsd:complexType name="SERVTOTAL_Type">
        <xsd:simpleContent>
            <xsd:extension base="xsd:string">
                <xsd:attribute type="xsd:string" name="voicePricePlan" use="optional"/>
                <xsd:attribute type="xsd:float" name="totalUsage" use="optional"/>
                <xsd:attribute type="xsd:float" name="totalCharges" use="optional"/>
                <xsd:attribute type="xsd:float" name="total" use="optional"/>
                <xsd:attribute type="xsd:string" name="serv" use="optional"/>
                <xsd:attribute type="xsd:float" name="netTotalUsage" use="optional"/>
                <xsd:attribute type="xsd:float" name="netTotalCharges" use="optional"/>
                <xsd:attribute type="xsd:float" name="netTotal" use="optional"/>
                <xsd:attribute type="xsd:byte" name="franchiseDiscount" use="optional"/>
                <xsd:attribute type="xsd:byte" name="franchiseCharge" use="optional"/>
                <xsd:attribute type="xsd:string" name="fixPricePlan" use="optional"/>
                <xsd:attribute type="xsd:string" name="fixExternalId" use="optional"/>
                <xsd:attribute type="xsd:string" name="extIdDOC1" use="optional"/>
