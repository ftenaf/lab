                <xsd:attribute type="xsd:string" name="sfid" use="optional"/>
                <xsd:attribute type="xsd:int" name="serv" use="optional"/>
                <xsd:attribute type="xsd:byte" name="productLine" use="optional"/>
                <xsd:attribute type="xsd:byte" name="productGroup" use="optional"/>
                <xsd:attribute type="xsd:float" name="amtCredited" use="optional"/>
                <xsd:attribute type="xsd:int" name="techTransDt" use="optional"/>
                <xsd:attribute type="xsd:byte" name="techFlag" use="optional"/>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    
    <xsd:complexType name="NOTAXSUMM_Type">
        <xsd:sequence>
            <xsd:element name="LINE" maxOccurs="unbounded" minOccurs="0" type="LINE_Type" />
        </xsd:sequence>
        <xsd:attribute type="xsd:byte" name="totalDiscount"/>
        <xsd:attribute type="xsd:float" name="total"/>
