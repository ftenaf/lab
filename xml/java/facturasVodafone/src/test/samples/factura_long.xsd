                <xsd:attribute type="xsd:string" name="descGCU"/>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    
    <xsd:complexType name="TAXSUMM_Type">
        <xsd:simpleContent>
            <xsd:extension base="xsd:string">
                <xsd:attribute type="xsd:float"  name="totalTax" use="optional"/>
                <xsd:attribute type="xsd:float"  name="totalAmt" use="optional"/>
                <xsd:attribute type="xsd:string" name="taxSap"   use="optional"/>
                <xsd:attribute type="xsd:float"  name="taxRate"  use="optional"/>
                <xsd:attribute type="xsd:string" name="taxName"  use="optional"/>
                <xsd:attribute type="xsd:float"  name="baseTotal" use="optional"/>
                <xsd:attribute type="xsd:float"  name="amtToPay" use="optional"/>
                <xsd:attribute type="xsd:float"  name="totalAmt2" use="optional"/>
                <xsd:attribute type="xsd:float"  name="totalAmt1" use="optional"/>
                <xsd:attribute type="xsd:string" name="taxSap2"  use="optional"/>
                <xsd:attribute type="xsd:string" name="taxSap1"  use="optional"/>
                <xsd:attribute type="xsd:float"  name="baseTotal2" use="optional"/>
                <xsd:attribute type="xsd:float"  name="baseTotal1" use="optional"/>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    <xsd:complexType name="groupTOT_Type" >
        <xsd:simpleContent>
            <xsd:extension base="xsd:string">
                <xsd:attribute type="xsd:float" name="totalUsg" use="optional"/>
                <xsd:attribute type="xsd:float" name="totalChrg" use="optional"/>
                <xsd:attribute type="xsd:float" name="totalAmt" use="optional"/>
                <xsd:attribute type="xsd:byte" name="groupId" use="optional"/>
                <xsd:attribute type="xsd:string" name="groupDesc" use="optional"/>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    <xsd:complexType name="TOT_Type">
        <xsd:sequence>
            <xsd:element name="TAXSUMM" maxOccurs="unbounded" minOccurs="0" type="TAXSUMM_Type" />
            <xsd:element name="groupTOT" maxOccurs="unbounded" minOccurs="0" type="groupTOT_Type" />
        </xsd:sequence>
        <xsd:attribute type="xsd:float" name="totalAmt" use="optional"/>
        <xsd:attribute type="xsd:float" name="subtotalAmt" use="optional"/>
        <xsd:attribute type="xsd:float" name="simPay" use="optional"/>
        <xsd:attribute type="xsd:float" name="previousDue" use="optional"/>
        <xsd:attribute type="xsd:float" name="pendingCredit" use="optional"/>
        <xsd:attribute type="xsd:float" name="paymentsAmt" use="optional"/>
        <xsd:attribute type="xsd:float" name="hogar" use="optional"/>
        <xsd:attribute type="xsd:float" name="gamb" use="optional"/>
        <xsd:attribute type="xsd:float" name="billCredit" use="optional"/>
        <xsd:attribute type="xsd:float" name="amtToPay" use="optional"/>
        <xsd:attribute type="xsd:float" name="TCVF" use="optional"/>
        <xsd:attribute type="xsd:float" name="ER" use="optional" />
        <xsd:attribute type="xsd:float" name="totalDiscount" use="optional"/>
        <xsd:attribute type="xsd:short" name="count" use="optional"/>
        <xsd:attribute type="xsd:int"   name="totalUnits" use="optional"/>
        <xsd:attribute type="xsd:float" name="appliedDiscount" use="optional"/>
        <xsd:attribute type="xsd:string" name="liDura" use="optional"/>
        <xsd:attribute type="xsd:short" name="unitsType" use="optional"/>
    </xsd:complexType>

    <xsd:complexType name="RESDISC_Type">
        <xsd:simpleContent>
            <xsd:extension base="xsd:string">
                <xsd:attribute type="xsd:float" name="totalDisc" use="optional"/>
                <xsd:attribute type="xsd:float" name="totalAmt" use="optional"/>
                <xsd:attribute type="xsd:string" name="desc" use="optional"/>
                <xsd:attribute type="xsd:byte" name="count" use="optional"/>
                <xsd:attribute type="xsd:short" name="discountId" use="optional"/>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    
    <xsd:complexType name="RESTYPE_Type">
        <xsd:sequence>
            <xsd:element name="TOT" type="TOT_Type" />
        </xsd:sequence>
        <xsd:attribute type="xsd:short" name="idTipo" use="optional"/>
        <xsd:attribute type="xsd:string" name="descTipo" use="optional"/>
    </xsd:complexType>

    <xsd:complexType name="RESGROUP_Type">
        <xsd:sequence>
            <xsd:element name="TOT" type="TOT_Type" />
            <xsd:element name="Rc" maxOccurs="unbounded" minOccurs="0" type="RC_Type" />
            <xsd:element name="NRC" type="RC_Type" />
            <xsd:element name="RESDISC" maxOccurs="unbounded" minOccurs="0" type="RESDISC_Type" />
            <xsd:element name="RESTYPE" maxOccurs="unbounded" minOccurs="0" type="RESTYPE_Type" />
        </xsd:sequence>
        <xsd:attribute type="xsd:byte" name="groupID" use="optional"/>
        <xsd:attribute type="xsd:string" name="groupDesc" use="optional"/>
    </xsd:complexType>
    
    <xsd:complexType name="RESCHA_Type">
        <xsd:sequence>
            <xsd:element name="TOT" type="TOT_Type" />
            <xsd:element name="RESGROUP" maxOccurs="unbounded" minOccurs="0" type="RESGROUP_Type" />
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="RESUSG_Type">
        <xsd:sequence>
            <xsd:element name="TOT" type="TOT_Type"/>
            <xsd:element name="RESGROUP" maxOccurs="unbounded" minOccurs="0" type="RESGROUP_Type" />
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="SERVSUMMARY_Type">
        <xsd:sequence>
            <xsd:element name="Tot" type="TOT_Type" />
            <xsd:element name="RESCHA" type="RESCHA_Type" />
            <xsd:element name="RESUSG" type="RESUSG_Type" />
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="CONT_Type">
        <xsd:simpleContent>
            <xsd:extension base="xsd:string">
                <xsd:attribute type="xsd:string" name="taxSap" use="optional"/>
                <xsd:attribute type="xsd:float" name="tax" use="optional"/>
                <xsd:attribute type="xsd:int" name="sfid" use="optional"/>
                <xsd:attribute type="xsd:float" name="percent" use="optional">
                    <xsd:annotation>
                        <xsd:appinfo>
                            <jaxb:property name="percentNo"/>
                        </xsd:appinfo>
                    </xsd:annotation>  
                </xsd:attribute>
                <xsd:attribute type="xsd:byte" name="flagComission" use="optional"/>
                <xsd:attribute type="xsd:float" name="disc" use="optional"/>
                <xsd:attribute type="xsd:string" name="contAcctDesc" use="optional"/>
                <xsd:attribute type="xsd:int" name="contAcct" use="optional"/>
                <xsd:attribute type="xsd:float" name="amt" use="optional"/>
                <xsd:attribute type="xsd:long" name="accalias" use="optional"/>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    
    <xsd:complexType name="DISC_Type">
        <xsd:simpleContent>
            <xsd:extension base="xsd:string">
                <xsd:attribute type="xsd:byte" name="suppServ" use="optional"/>
                <xsd:attribute type="xsd:date" name="startDt" use="optional"/>
                <xsd:attribute type="xsd:byte" name="productLineId" use="optional"/>
                <xsd:attribute type="xsd:byte" name="franchFlag" use="optional"/>
                <xsd:attribute type="xsd:float" name="federalTax" use="optional"/>
                <xsd:attribute type="xsd:short" name="discountId" use="optional"/>
                <xsd:attribute type="xsd:byte" name="discType" use="optional"/>
                <xsd:attribute type="xsd:short" name="billRow" use="optional"/>
                <xsd:attribute type="xsd:float" name="baseAmt" use="optional"/>
                <xsd:attribute type="xsd:byte" name="subscrNoResets" use="optional"/>
                <xsd:attribute type="xsd:byte" name="subscrNo" use="optional"/>
                <xsd:attribute type="xsd:byte" name="productGroupID" use="optional"/>
                <xsd:attribute type="xsd:byte" name="icbAllow" use="optional"/>
                <xsd:attribute type="xsd:short" name="descriptionCode" use="optional"/>
                <xsd:attribute type="xsd:float" name="tax" use="optional"/>
                <xsd:attribute type="xsd:float" name="discPercent" use="optional"/>
                <xsd:attribute type="xsd:float" name="disc" use="optional"/>
                <xsd:attribute type="xsd:float" name="amt" use="optional"/>
                <xsd:attribute type="xsd:byte" name="draw" use="optional"/>
                <xsd:attribute type="xsd:string" name="desc" use="optional"/>
                <xsd:attribute type="xsd:byte" name="productLineFPGD" use="optional"/>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>    
    
    <xsd:complexType name="RCS_Type">
        <xsd:sequence>
            <xsd:element name="TOT" type="TOT_Type" />
            <xsd:element name="RC" maxOccurs="unbounded" minOccurs="0" type="RC_Type" />
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="DISCS_Type" >
        <xsd:sequence>
            <xsd:element name="TOT" type="TOT_Type" />
            <xsd:element name="DISC" maxOccurs="unbounded" minOccurs="0" type="DISC_Type" />
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="ACCNTCHARGES_Type">
        <xsd:sequence>
            <xsd:element name="Tot" type="TOT_Type" />
            <xsd:element name="RCS" type="RCS_Type" />
            <xsd:element name="DISCS" type="DISCS_Type" />
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="PULL_Type">
        <xsd:simpleContent>
            <xsd:extension base="xsd:string">
                <xsd:attribute type="xsd:string" name="descResp"/>
                <xsd:attribute type="xsd:string" name="descOrden"/>
                <xsd:attribute type="xsd:string" name="descDept"/>
                <xsd:attribute type="xsd:byte" name="codResp"/>
                <xsd:attribute type="xsd:byte" name="codDept"/>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    <xsd:complexType name="OPT_Type">
        <xsd:sequence>
            <xsd:element name="HIST" type="HIST_Type" />
            <xsd:element name="LOY" type="LOY_Type" />
            <xsd:element name="PULL" type="PULL_Type" />
            <xsd:element name="VPN" type="VPN_Type" />
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="INFO_Type">
        <xsd:sequence>
            <xsd:element name="BILL" type="BILL_Type" />
            <xsd:element name="CUST" type="CUST_Type" />
            <xsd:element name="ADDR" type="ADDR_Type" />
            <xsd:element name="PAY"  type="PAY_Type"/>
            <xsd:element name="MK" type="MK_Type" />
            <xsd:element name="OPT" minOccurs="0" type="OPT_Type" />
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="EXEMPT_Type">
        <xsd:simpleContent>
            <xsd:extension base="xsd:string">
                <xsd:attribute type="xsd:string" name="type" use="optional"/>
                <xsd:attribute type="xsd:string" name="time" use="optional"/>
                <xsd:attribute type="xsd:string" name="subtype" use="optional"/>
                <xsd:attribute type="xsd:int" name="serv" use="optional"/>
                <xsd:attribute type="xsd:string" name="product" use="optional"/>
                <xsd:attribute type="xsd:string" name="desc" use="optional"/>
                <xsd:attribute type="xsd:date" name="date" use="optional"/>
                <xsd:attribute type="xsd:float" name="amt" use="optional"/>
                <xsd:attribute type="xsd:string" name="ShopName" use="optional"/>
                <xsd:attribute type="xsd:short" name="ProviderId" use="optional"/>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    
    <xsd:complexType name="LINE_Type">
        <xsd:sequence>
            <xsd:element name="EXEMPT" type="EXEMPT_Type"/>
