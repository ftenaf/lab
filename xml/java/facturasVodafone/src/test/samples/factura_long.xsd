    <xsd:complexType name="CREDIT_Type">
        <xsd:simpleContent>
            <xsd:extension base="xsd:string">
                <xsd:attribute type="xsd:short" name="unitCrId" use="optional"/>
                <xsd:attribute type="xsd:int" name="subscrSeq" use="optional"/>
                <xsd:attribute type="xsd:byte" name="subscrNoResets" use="optional"/>
